package ConversorMoneda;

import Errores.ErroresMoneda;
import Servicio.MonedaServicio;

public class ConversorMoneda {

	public static void main(String[] args) throws ErroresMoneda {
		
		MonedaServicio ms = new MonedaServicio();
		
		double peso = 25;
		
		System.out.println("DOLAR: " + ms.dePesoADolar(peso));
		System.out.println("EURO: " + ms.dePesoAEuro(peso));
		System.out.println("LIBRAS: " + ms.dePesoALibrasEsterlinas(peso));
		System.out.println("WON: " + ms.dePesoAWonSurcoreano(peso));
		System.out.println("YEN: " + ms.dePesoAYenJapones(peso));
		System.out.println("-----------------");
		System.out.println("DOLAR A PESO: " + ms.deDolarAPeso(peso));
		System.out.println("EURO A PESO: " + ms.deEuroAPeso(peso));
		System.out.println("LIBRAS A PESO: " + ms.deLibrasEsterlinasAPeso(peso));
		System.out.println("WON A PESO: " + ms.deWonSurcoreanoAPeso(peso));
		System.out.println("YEN A PESO: " + ms.deYenJaponesAPeso(peso));
	}

}
