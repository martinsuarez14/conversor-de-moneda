package ConversorMoneda;

import Servicio.MonedaServicio;

public class ConversorMoneda {

	public static void main(String[] args) {
		
		MonedaServicio ms = new MonedaServicio();
		
		double peso = 25;
		
		System.out.println("DOLAR: " + ms.dePesoADolar(peso));
		System.out.println("EURO: " + ms.dePesoAEuro(peso));
		System.out.println("LIBRAS: " + ms.dePesoALibrasEsterlinas(peso));
		System.out.println("WON: " + ms.dePesoAWonSurcoreano(peso));
		System.out.println("YEN: " + ms.dePesoAYenJapones(peso));

		
	}

}
